  FormBox[
   RowBox[{
    SubscriptBox["m", "o"], "-", 
    RowBox[{"d", " ", "\[Sigma]"}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"931ffd7e-f4c4-471a-9904-3ec98a9b089d"],
